<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:web="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <servlet>
    <servlet-name>Name1</servlet-name>
    <servlet-class>com.hello.Hello</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Name2</servlet-name>
    <servlet-class>com.hello.Second</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Name1</servlet-name>
    <url-pattern>/hi</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Name2</servlet-name>
    <url-pattern>/sec</url-pattern>
  </servlet-mapping>
  <context-param>
    <param-name>myvalue</param-name>
    <param-value> verizon in palmeto</param-value>
  </context-param>
</web-app>